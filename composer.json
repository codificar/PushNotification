{
    "name": "codificar/push-notificationfcm",
    "description": "Laravel Package to send push notifications to Android and IOS devices. (GCM,FCM,APN)",
    "keywords": [
        "Laravel",
        "push",
        "notification",
        "gcm",
        "apn",
        "fcm",
        "high",
        "priority"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/codificar/PushNotification/issues",
        "source": "https://github.com/codificar/PushNotification"
    },
    "require": {
        "php": "^7.1.3 || ^8.0",
        "guzzlehttp/guzzle": "^6.3 || ^7.0.1",
        "illuminate/support": "~5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/notifications": "~5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "google/apiclient": "^2.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0  || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codificar\\PushNotificationFcm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codificar\\PushNotificationFcm\\Providers\\PushNotificationServiceProvider"
            ],
            "aliases": {
                "PushNotificationFcm": "Codificar\\PushNotificationFcm\\Facades\\PushNotification"
            }
        }
    }
}